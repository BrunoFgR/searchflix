{
  "name": "searchflix",
  "version": "1.0.0",
  "description": "An API that manage movies",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:server": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only src/shared/infra/http/server.ts",
    "migration:generate": "npx sequelize-cli migration:generate --name",
